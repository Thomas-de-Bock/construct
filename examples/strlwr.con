extern printf

section .text
function strlwr(str):
	while byte[str] ne 0:
		if byte[str] ge 65:
			if byte[str] le 90:
				!crntchr sil
				mov crntchr, byte[str]
				add crntchr, 32
				mov byte[str], crntchr
		inc str


function main():
	call strlwr(teststring)
	call printf(fmt, teststring)

	mov rax, 60
	syscall

section .data
teststring db "HeLlO WoRlD", 0
fmt: db "%s", 10, 0


extern printf

section .text
function strlwr(str):
	!A_letter 65
	!Z_letter 90
	!end_of_str 0
	!A_to_a 32
	while byte[str] ne end_of_str:
		if byte[str] ge A_letter:
			if byte[str] le Z_letter:
				!crntchr sil
				mov crntchr, byte[str]
				add crntchr, A_to_a
				mov byte[str], crntchr
		inc str


function main():
	call strlwr(teststr)
	call printf(fmt, teststr)

	syscall exit()

section .data
teststr: db "HeLlO WoRlD", 0
fmt: db "%s", 10, 0


extern printf

section .text
function strchr(str, chr):
	!ptrresult rax
	!findchr sil
	!end_of_str 0
	!nullptr 0
	mov ptrresult, nullptr
	while byte[str] ne end_of_str:
		if byte[str] e findchr:
			mov ptrresult, str
			ret
		inc str

function main():
	!W_letter 87
	call strchr(teststr, W_letter)
	!result rax
	call printf(fmt, result)
	call printf(fmt, teststr)

	syscall exit()

section .data
teststr: db "Hello World!", 0
fmt: db "%p", 10, 0


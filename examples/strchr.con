extern printf

section .text
function strchr(str, chr):
	!ptrresult rax
	mov ptrresult, 0
	while byte[str] ne 0:
		if byte[str] e chr:
			mov ptrresult, str
			ret
		inc str

function main():
	call strchr(teststr, 87)
	!result rax
	call printf(fmt, result)
	call printf(fmt, teststr)

	mov rax, 60
	syscall

section .data
fmt: db "%p", 10, 0
teststr: db "Hello World!", 0

